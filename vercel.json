{
  "version": 2,
  "routes": [
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/register", 
      "dest": "/api/register.js"
    },
    {
      "src": "/api/logout",
      "dest": "/api/logout.js"
    },
    {
      "src": "/api/spots",
      "dest": "/api/spots.js"
    },
    {
      "src": "/api/bookings",
      "dest": "/api/bookings.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}